{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-undef": "off",
    "no-unused-vars": "off"
  },
  "overrides": [
    {
      "files": ["**/*.ejs"],
      "rules": {
        "no-undef": "off",
        "no-unused-vars": "off",
        "no-undef": "off"
      }
    }
  ],
  "ignorePatterns": ["**/*.ejs"]
}
